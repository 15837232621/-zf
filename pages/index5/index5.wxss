
.name{
  font-family: PingFang-SC-Bold;
	font-size: 48rpx;
	letter-spacing: 2rpx;
	color: #333333;
}
.myname{
  font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	letter-spacing: 1rpx;
	color: #999999;
  margin-top: 18rpx;
}
.myimage{
  width: 125rpx;
	height: 125rpx;
  border-radius: 50%;
  overflow: hidden;
}
.message{
  padding:19rpx 54rpx 0 51rpx;
}
.dream{
  padding: 0 22rpx;
  margin-top: 69rpx;
}
.dream-city{
  width: 332rpx;
	height: 131rpx;
	background-color: #ffffff;
	box-shadow: 0px 4rpx 21rpx 0rpx 
		rgba(127, 127, 127, 0.12);
	border-radius: 10rpx;
    position: relative;
}
.dream-compony{
  width: 332rpx;
	height: 131rpx;
	background-color: #ffffff;
	box-shadow: 0px 4rpx 21rpx 0rpx 
		rgba(127, 127, 127, 0.12);
	border-radius: 10rpx;
  position: relative;
}
.icon_city3,.icon_plane {
  width: 53rpx;
	height: 50rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}

.dream-city-target{
  font-family: PingFang-SC-Medium;
	font-size: 26rpx;
	letter-spacing: 1rpx;
	color: #c4c4c4;
  margin-top: 24rpx;
}
.dream-city-text{
  font-family: PingFang-SC-Medium;
	font-size: 30rpx;
	letter-spacing: 1rpx;
	color: #333333;
  margin-top: 20rpx;
}
.dream-total{
  margin-left: 40rpx;
}
.tab-image{
  width:710rpx;
	height: 296rpx;
	border-radius: 20rpx;
  margin: 37rpx 0 20rpx 20rpx;
}